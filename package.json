{
  "name": "bunchee",
  "version": "1.1.1",
  "description": "simple bundler",
  "bin": {
    "bunchee": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "test": "jest",
    "clean": "rm -rf ./dist",
    "prerelease": "yarn clean && yarn build && chmod +x dist/cli.js",
    "build:cli": "bunchee src/cli.ts -f cjs -b -o dist/cli.js",
    "build:main": "bunchee src/index.ts -f cjs",
    "build": "yarn build:main && yarn build:cli"
  },
  "keywords": [
    "bundler"
  ],
  "files": [
    "dist",
    "*.md"
  ],
  "author": "huozhi (github.com/huozhi)",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-json": "4.0.2",
    "@rollup/plugin-node-resolve": "8.1.0",
    "@rollup/plugin-typescript": "3.1.1",
    "commander": "2.20.3",
    "rollup": "1.32.0",
    "rollup-plugin-commonjs": "10.1.0",
    "tslib": "2.0.0",
    "typescript": "^3.9.6"
  },
  "peerDependencies": {
    "typescript": ">= 3.4.0"
  },
  "devDependencies": {
    "bunchee": "1.1.0",
    "jest": "26.6.1"
  }
}
